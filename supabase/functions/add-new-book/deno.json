{
  "imports": {
    "@supabase/supabase-js": "npm:@supabase/supabase-js@2",
    "fflate": "npm:fflate@0.8.1",
    "fast-xml-parser": "npm:fast-xml-parser@4.3.4",
    "dompurify": "npm:dompurify@3.0.8"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
    "allowJs": true,
    "strict": true
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true,
    "proseWrap": "preserve"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  }
}
